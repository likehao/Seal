<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">

    <cn.fengwoo.sealsteward.view.ReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!--第一个卡片开始！！！！！！！！！！！！！！！！！！！！！！！！！-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingTop="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="5dp"
                            android:text="@string/seal_manage"
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/press_time_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/press_time" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/start_psd_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/start_password" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/key_psd_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/key_password" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/reset_device_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/reset_device" />

                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/Electronic_fence" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/Approval_flow" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout style="@style/add" />

                        <RelativeLayout style="@style/add" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingTop="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="5dp"
                            android:text="@string/resources_manage"
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/organizational_structure_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/organizational" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/add_person_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/add_person" />

                            </LinearLayout>
                        </RelativeLayout>


                        <RelativeLayout
                            android:id="@+id/add_seal_rl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/add_seal" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout style="@style/add" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
            <!--第二个卡片开始！！！！！！！！！！！！！！！！！！！！！！！！！-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingTop="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="5dp"
                            android:text="@string/cost_manage"
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/service_cost_reset" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    style="@style/cycleApplication"
                                    android:src="@drawable/ic_launcher" />

                                <TextView
                                    style="@style/application_text"
                                    android:text="@string/order" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                        </RelativeLayout>

                        <RelativeLayout style="@style/add" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
            <!--第二个卡片结束！！！！！！！！！！！！！！！！！！！！！！！！！-->

        </LinearLayout>

    </cn.fengwoo.sealsteward.view.ReboundScrollView>

</LinearLayout>
