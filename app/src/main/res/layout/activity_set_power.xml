<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    tools:context=".activity.SetPowerActivity">

    <include layout="@layout/head" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:orientation="vertical">

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="添加印章" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission1"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="编辑印章信息" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission2"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="删除印章" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission3"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置印章长按时间" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission4"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置盖章延时时间" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission5"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置印章启动密码" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission6"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置印章按键密码" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission7"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置印章语音开关" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission8"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="重置印章" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission9"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="添加脱机用户" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission10"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置脱机用户权限" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission11"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="删除脱机用户" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission12"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置电子围栏" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission13"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置审批流" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission14"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="添加用户" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission15"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="删除用户" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission16"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="添加用户权限" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission17"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="设置用户权限" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission18"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="查询公司人员盖章记录" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission19"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="添加组织架构" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission20"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="编辑组织架构" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission21"
                    style="@style/set_power_bt" />
            </RelativeLayout>

            <View style="@style/line_left" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="删除组织架构" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission22"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="移动用户或印章到其他部门权限" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission23"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="固件升级权限" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission24"
                    style="@style/set_power_bt" />

            </RelativeLayout>





            <View style="@style/line" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="更换印章" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission25"
                    style="@style/set_power_bt" />

            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout style="@style/set_power_rl">

                <TextView
                    style="@style/set_power_tv"
                    android:text="查看公司组织架构" />

                <com.suke.widget.SwitchButton
                    android:id="@+id/permission26"
                    style="@style/set_power_bt" />

            </RelativeLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>